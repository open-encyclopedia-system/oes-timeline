{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "oes/timeline-single",
  "version": "0.1.0",
  "title": "Timeline for Post",
  "category": "oes-schema",
  "description": "Display the OES timeline for a post according to timeline settings.",
  "keywords": ["event", "date", "presentation"],
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "textAlign": true
    },
    "color": {
      "text": true,
      "background": true,
      "link": true
    },
    "align": true,
    "anchor": true
  },
  "attributes": {
    "detail": {
      "type": "boolean",
      "default": false
    },
    "labels": {
      "type": "object",
      "default": {
        "start": "Start",
        "end": "End"
      }
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "render": "file:./render.php"
}
